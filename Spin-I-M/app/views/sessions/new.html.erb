<h1>Log in</h1>

<div class="row">
  <%= form_for :session  do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Login To Spin-I-M" %>
  <% end %>

  <p>New user? <%= link_to "Sign Up Instead!", new_user_path %></p>
</div>
